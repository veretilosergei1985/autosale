                                                                                                                                                    function pluralForm(n, form1, form2, form5) {
                                                                                                                                                        n = n % 100;
                                                                                                                                                        n1 = n % 10;
                                                                                                                                                        if (n > 10 && n < 20)
                                                                                                                                                            return form5;
                                                                                                                                                        if (n1 > 1 && n1 < 5)
                                                                                                                                                            return form2;
                                                                                                                                                        if (n1 == 1)
                                                                                                                                                            return form1;
                                                                                                                                                        return form5;
                                                                                                                                                    }

                                                                                                                                                    function select_tab(item) {
                                                                                                                                                        if (!$$('#fishki-ads-' + item).length)
                                                                                                                                                            return false;

                                                                                                                                                        $$('.fishki-ads').hide();
                                                                                                                                                        $$('#fishki-ads-' + item).show();
                                                                                                                                                        $$('.raise-menu>li').removeClass('active');
                                                                                                                                                        if (item == 'tab1') {
                                                                                                                                                            $$('.fishki-tab-1').addClass('active');
                                                                                                                                                            $$('.fishki-tab-2').addClass('b-bt');
                                                                                                                                                            $$('.fishki-tab-2').removeClass('b-top');
                                                                                                                                                        } else if (item == 'tab2') {
                                                                                                                                                            $$('.fishki-tab-2').addClass('active');
                                                                                                                                                            $$('.fishki-tab-2').removeClass('b-top');
                                                                                                                                                            $$('.fishki-tab-2').removeClass('b-bt');
                                                                                                                                                            Cookie.write('fishki_popup_view', item);
                                                                                                                                                        } else if (item == 'tab3') {
                                                                                                                                                            $$('.fishki-tab-3').addClass('active');
                                                                                                                                                            $$('.fishki-tab-2').addClass('b-top');
                                                                                                                                                            $$('.fishki-tab-2').removeClass('b-bt');
