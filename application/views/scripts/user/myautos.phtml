                                                                                                                                                                                                            function auto_saled(auto_id, saled) {
                                                                                                                                                                                                                var pop_up = $('autoDeleteReason' + auto_id);
                                                                                                                                                                                                                var saled_form = pop_up.getElementById('saled-form-' + auto_id);
                                                                                                                                                                                                                var item_info = $(auto_id + '_itemHeaderLink').get('title');
                                                                                                                                                                                                                if (saled) {
                                                                                                                                                                                                                    if (saled_form.get('saled') == 0) {
                                                                                                                                                                                                                        pop_up.getElementById('reason-title-' + auto_id).set('text', 'Сообщить о продаже авто');
                                                                                                                                                                                                                        pop_up.getElementById('reason-reason-' + auto_id).set('html', 'Пожалуйста, укажите способ продажи Вашего автомобиля <strong>' + item_info + '<strong>:');
                                                                                                                                                                                                                        pop_up.getElementById('repub-' + auto_id).setStyle('display', 'none');
                                                                                                                                                                                                                        pop_up.getElementById('changed-' + auto_id).setStyle('display', 'none');
                                                                                                                                                                                                                        $('complaintAdvertisementForm-' + auto_id).removeEvents('submit');

                                                                                                                                                                                                                        $('complaintAdvertisementForm-' + auto_id).addEvent('submit', function(e) {
                                                                                                                                                                                                                            e.stop();
                                                                                                                                                                                                                            var req = new Request({
                                                                                                                                                                                                                                url: '/ajax.php',
                                                                                                                                                                                                                                data: $('complaintAdvertisementForm-' + auto_id),
                                                                                                                                                                                                                                onSuccess: function() {
                                                                                                                                                                                                                                    $('complaintAdvertisementForm-' + auto_id).reset();
                                                                                                                                                                                                                                    saled_form.set('saled', '1');
                                                                                                                                                                                                                                    saled_form.setStyle('display', 'none');
                                                                                                                                                                                                                                    pop_up.getElementById('succes-remove-' + auto_id).setStyle('display', 'block');
                                                                                                                                                                                                                                }
                                                                                                                                                                                                                            }).send();

                                                                                                                                                                                                                        });
                                                                                                                                                                                                                    } else {
                                                                                                                                                                                                                        saled_form.setStyle('display', 'none');
                                                                                                                                                                                                                        pop_up.getElementById('succes-remove-' + auto_id).setStyle('display', 'block');
                                                                                                                                                                                                                    }
                                                                                                                                                                                                                } else {
                                                                                                                                                                                                                    saled_form.setStyle('display', 'block');
                                                                                                                                                                                                                    pop_up.getElementById('succes-remove-' + auto_id).setStyle('display', 'none');

                                                                                                                                                                                                                    pop_up.getElementById('reason-title-' + auto_id).set('text', 'Удаление объявления');
                                                                                                                                                                                                                    pop_up.getElementById('reason-reason-' + auto_id).set('html', 'Пожалуйста, укажите причину, по которой Вы удаляете объявление <strong>' + item_info + '<strong>:');
                                                                                                                                                                                                                    pop_up.getElementById('repub-' + auto_id).setStyle('display', 'block');
                                                                                                                                                                                                                    pop_up.getElementById('changed-' + auto_id).setStyle('display', 'block');
                                                                                                                                                                                                                    $('complaintAdvertisementForm-' + auto_id).removeEvents('submit');
                                                                                                                                                                                                                }
                                                                                                                                                                                                                fToggle(auto_id)
                                                                                                                                                                                                            }

                                                                                                                                                                                                            function fToggle(id) {
                                                                                                                                                                                                                var pop_up = $('autoDeleteReason' + id);
                                                                                                                                                                                                                if (pop_up.getStyle('display') == 'block') {
                                                                                                                                                                                                                    pop_up.setStyle('display', 'none');
                                                                                                                                                                                                                } else {
                                                                                                                                                                                                                    pop_up.setStyle('display', 'block');
                                                                                                                                                                                                                }
                                                                                                                                                                                                            }
                                                                                                                                                                                                            //функция для показа или скрывания дополнительного описания уведомления
                                                                                                                                                                                                            function notificationHideOrShow(id_notification) {
                                                                                                                                                                                                                if ($('description_' + id_notification).getStyle('display') == "none") {
                                                                                                                                                                                                                    $('description_' + id_notification).setStyle('display', 'block');
                                                                                                                                                                                                                    $('show_' + id_notification).set('html', '<em class="btn larr"></em>скрыть');
                                                                                                                                                                                                                    $('show_' + id_notification).set('title', 'Скрыть подробную информацию');
                                                                                                                                                                                                                } else {
                                                                                                                                                                                                                    $('show_' + id_notification).set('html', '<em class="btn larr"></em>развернуть');
                                                                                                                                                                                                                    $('show_' + id_notification).set('title', 'Показать подробную информацию');
                                                                                                                                                                                                                    $('description_' + id_notification).setStyle('display', 'none');
                                                                                                                                                                                                                }

                                                                                                                                                                                                            }
